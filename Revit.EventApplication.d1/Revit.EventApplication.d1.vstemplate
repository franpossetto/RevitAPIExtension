<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <Name>Revit Event Application (d1)</Name>
    <Description>Create a Revit Application associated to DocumentOpened Event Handler</Description>
    <Icon>Revit.EventApplication.d1.png</Icon>
	  <ProjectType>CSharp</ProjectType>
	  <LanguageTag>csharp</LanguageTag>
	  <ProjectTypeTag>Revit</ProjectTypeTag>
	  <ProjectTypeTag>RevitAPI</ProjectTypeTag>
	  <PlatformTag>.NET Framework</PlatformTag>
	  <PlatformTag>Windows</PlatformTag>
    <RequiredFrameworkVersion>2.0</RequiredFrameworkVersion>
    <SortOrder>1000</SortOrder>
    <TemplateID>6f94d71e-f0f7-4fb3-ac74-2a33cf5e6c34</TemplateID>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Revit_Event_Application</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
  </TemplateData>
  <TemplateContent>
    <Project File="ProjectTemplate.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="Properties\AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
	  <ProjectItem ReplaceParameters="true" TargetFileName="Properties\Resources.Designer.cs">Resources.Designer.cs</ProjectItem>
	  <ProjectItem ReplaceParameters="true" TargetFileName="Properties\Resources.resx">Resources.resx</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="true">ExternalApplication.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="false">$safeprojectname$.addin</ProjectItem>
	  <ProjectItem ReplaceParameters="true" TargetFileName="Resources\icon.png">icon.png</ProjectItem>
    </Project>
  </TemplateContent>
	<WizardExtension>
		<Assembly>RevitAPIExtension, Version=1.0.0.0, Culture=Neutral, PublicKeyToken=null</Assembly>
		<FullClassName>RevitAPIExtension.Wizards.VersionWizard</FullClassName>
	</WizardExtension>
</VSTemplate>