		<Imports>
			<Import>
				<Namespace>Autodesk.Revit.DB</Namespace>
			</Import>
		</Imports>
    <Code Language="CSharp">
        <![CDATA[
        ProjectInfo pi = commandData.Application.ActiveUIDocument.Document.ProjectInformation;
        ]]>
    </Code>
    </Snippet>
