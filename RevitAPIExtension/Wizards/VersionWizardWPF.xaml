<Window x:Class="RevitAPIExtension.Wizards.VersionWizardWPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RevitAPIExtension.Wizards"
             mc:Ignorable="d" Title="Revit API Version Selector" Height="238" Width="379" WindowStartupLocation="CenterScreen">
    <Grid Margin="10,32,2,7">
        <ComboBox x:Name="RevitAPIVerCB" HorizontalAlignment="Left" Margin="55,50,0,0" VerticalAlignment="Top" Width="130" Height="20">
            <ComboBoxItem Content="2017"/>
            <ComboBoxItem Content="2018"/>
            <ComboBoxItem Content="2019"/>
            <ComboBoxItem Content="2020"/>
            <ComboBoxItem Content="2021"/>
        </ComboBox>
        <Button x:Name="Accept" Content="Accept"  HorizontalAlignment="Left" Margin="210,100,0,0" VerticalAlignment="Top" Width="99" Height="23" Click="Accept_Click"/>
    </Grid>
</Window>
